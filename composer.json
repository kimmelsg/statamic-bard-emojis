{
  "require": {
    "php": "^8.2",
    "statamic/cms": "^3.4"
  },
  "name": "kimmelsg/statamic-bard-emojis",
  "keywords": ["statamic", "bard", "emojis", "emoticons"],
  "description": "Statamic Bard Emojis",
  "autoload": {
    "psr-4": {
      "Kimmelsg\\BardEmojis\\": "src"
    }
  },
  "type": "statamic-addon",
  "extra": {
    "statamic": {
      "name": "Statamic Bard Emojis",
      "description": "Add emoji suport for Bard."
    },
    "laravel": {
      "providers": ["Kimmelsg\\BardEmojis\\ServiceProvider"]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pixelfear/composer-dist-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
